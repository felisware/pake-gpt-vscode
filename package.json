{
  "main": "extension.js",
  "name": "pake-gpt",
  "icon": "icon.png",
  "displayName": "Pake GPT",
  "description": "Analisa, Buat, Perbaiki, dan Terangkan kode secara cepat serta akurat menggunakan AI GPT",
  "version": "1.0.0",
  "pricing": "Free",
  "license": "SEE LICENSE IN LICENSE.md",
  "publisher": "Pake GPT",
  "repository": {
    "type": "git",
    "url": "https://github.com/felisware/pake-gpt-vscode.git"
  },
  "categories": [
    "Snippets",
    "Formatters",
    "Education",
    "Machine Learning"
  ],
  "keywords": [
    "pake gpt",
    "code generator",
    "gpt",
    "chat gpt",
    "code gpt"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "engines": {
    "vscode": "^1.1.37"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "analisa",
        "title": "Analisa"
      },
      {
        "command": "buat",
        "title": "Buat"
      },
      {
        "command": "buat.disamping",
        "title": "Buat di samping"
      },
      {
        "command": "terangkan",
        "title": "Terangkan"
      },
      {
        "command": "perbaiki",
        "title": "Perbaiki"
      },
      {
        "command": "perbaiki.disamping",
        "title": "Perbaiki di samping"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection && editorTextFocus",
          "submenu": "pakegpt.submenu",
          "group": "navigation"
        }
      ],
      "pakegpt.submenu": [
        {
          "command": "analisa"
        },
        {
          "command": "buat"
        },
        {
          "command": "buat.disamping"
        },
        {
          "command": "terangkan"
        },
        {
          "command": "perbaiki"
        },
        {
          "command": "perbaiki.disamping"
        }
      ]
    },
    "submenus": [
      {
        "id": "pakegpt.submenu",
        "label": "Pake GPT"
      }
    ],
    "configuration": {
      "title": "Pake GPT",
      "properties": {
        "pakegpt.token": {
